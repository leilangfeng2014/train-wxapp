.care{
    margin: 15px 28px;
}
.picker{
    text-align: center;
}
.student{
    width: 100%;
    height: 110px;
    background: rgba(0,131,255,0.20);
    border-radius: 8px;
    display: flex;
    text-align: center;
    margin-bottom: 10px;
}
.student .cir{
    margin-top: 15px;
}
.student .cirText{
    top:40px;
    font-family: PingFangSC-Semibold;
    font-size: 12px;
    color: #0083FF;
    letter-spacing: 0.31px;
    text-align: center;
}
.student .cirText button{
    font-family: PingFangSC-Regular;
    font-size: 12px;
    color: #FFFFFF;
    width: 65px;
    height: 24px;
    line-height: 24px;
    background: #0083FF;
    border-radius: 3px;
    margin-top: 5px;
}
.process{
    flex: 4;
    position: relative;
}
.stext{
    flex: 5;
    margin-top: 12px;
}
.stext .name{
    font-family: PingFangSC-Regular;
    font-size: 12px;
    color: #0083FF;
    text-align: left;
    font-size: 16px;
    line-height: 22px;
}
.stext .time{
    margin-top: 6px;
    font-family: PingFangSC-Semibold;
    font-size: 14px;
    color: #0083FF;
    text-align: left;
    line-height: 20px;
}